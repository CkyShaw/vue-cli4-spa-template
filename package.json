{
	"name": "vue-cli4-spa-template",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"dev": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"format": "prettier --write \"src/**/*.js\" \"src/**/*.vue\" \"src/**/*.json\"",
		"prettier": "prettier --write \"src/**/*.{js,jsx,ts,json,vue,css,less,scss,md,html}\""
	},
	"dependencies": {
		"ant-design-vue": "^1.6.2",
		"axios": "^0.19.2",
		"core-js": "^3.6.5",
		"echarts": "^4.8.0",
		"element-ui": "^2.13.2",
		"highcharts": "^8.1.0",
		"moment": "^2.26.0",
		"mqtt": "^4.1.0",
		"pinyin2": "^2.0.8",
		"view-design": "^4.2.0",
		"vue": "^2.6.11",
		"vue-router": "^3.3.2",
		"vuex": "^3.4.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.4.1",
		"@vue/cli-plugin-eslint": "~4.4.1",
		"@vue/cli-plugin-router": "~4.4.1",
		"@vue/cli-plugin-vuex": "~4.4.1",
		"@vue/cli-service": "~4.4.1",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-import": "^1.13.0",
		"compression-webpack-plugin": "^4.0.0",
		"eslint": "^7.1.0",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-vue": "^6.2.2",
		"less": "^3.11.2",
		"less-loader": "^6.1.0",
		"lint-staged": "^10.2.8",
		"mockjs": "^1.1.0",
		"postcss-pxtorem": "^5.1.1",
		"prettier": "^2.0.5",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
		"vue-template-compiler": "^2.6.11"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8",
		"chrome >= 38"
	],
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{json,css,less,scss,md,html}": [
			"prettier --write",
			"git add"
		],
		"*.{js,jsx,ts,vue}": [
			"vue-cli-service lint",
			"git add"
		]
	}
}
